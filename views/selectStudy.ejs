<!--模板-->
<script type="text/template" id="liebiaomoban">
    <div class="table-responsive">
        <button type="button" class="btn btn-primary btn-lg" data-toggle="modal" id="tianjiayonghuBtn"  data-target=".tianjiayonghuModel" >
            添加用户
        </button>
        <form enctype="multipart/form-data" method="post" enctype="multipart/form-data" action= {{= importUrl}} id="caozuomokuai">
            <input id="file-0a" class="file" type="file" multiple data-min-file-count="1" name="excel">
            <br>
            <button type="submit" class="btn btn-primary">上传</button>
            <button type="reset" class="btn btn-default">取消</button>
        </form>
        <table class="table table-striped">
            <thead>
            <tr>
                {{ for(var i = 0 ; i < result.length ; i++){}}
                <th>{{=result[i] }}</th>
                {{ } }}
            </tr>
            </thead>
            <tbody>
            {{ if (importUrl != "/nlyy/dcsj"){ }}
            {{ for(var i = 0 ; i < data.length ; i++){ }}
            <tr id="neirongTr">
                {{ for(var j = 0 ; j < resultEn.length ; j++){ }}
                {{ if(resultEn[j] == '操作' ){ }}
                {{ var dataId = data[i].id + ',' + data[i].StudyID; }}
                <td><a data-toggle="modal" data-target=".caozuoModel" name= {{= dataId}} onclick="t(this.name)">操作</a></td>
                {{ }else if(resultEn[j] == 'Date'){ }}
                {{ var date = new Date(data[i][resultEn[j]]); }}
                {{ var dateStr =  date.getFullYear()+"-"+(date.getMonth()+1)+"-"+date.getDate();}}
                <td>{{= dateStr}}</td>
                {{ }else {}}
                <td>{{= data[i][resultEn[j]]}}</td>
                {{ } }}
                {{ } }}
            </tr>
            {{ } }}
            {{ }else{ }}
            {{ for(var i = 0 ; i < data.length ; i++){ }}
            <tr id="neirongTr">
                {{ for(var j = 0 ; j < resultEn.length ; j++){ }}
                {{ if(resultEn[j] == '操作' ){ }}
                {{ var dataId = data[i].id + ',' + data[i].StudyID; }}
                <td><a data-toggle="modal" data-target=".daochushujuModel" name= {{= dataId}} onclick="daochuFun(this.name)">选择</a></td>
                {{ }else if(resultEn[j] == 'Date'){ }}
                {{ var date = new Date(data[i][resultEn[j]]); }}
                {{ var dateStr =  date.getFullYear()+"-"+(date.getMonth()+1)+"-"+date.getDate();}}
                <td>{{= dateStr}}</td>
                {{ }else {}}
                <td>{{= data[i][resultEn[j]]}}</td>
                {{ } }}
                {{ } }}
            </tr>
            {{ } }}
            {{ } }}
            </tbody>
        </table>
        <nav>
            <ul class="pagination">
                {{ for(var i = 1 ; i <= pageCount ; i++){}}
                <li class="yemaanniu" data-page={{= i.toString() }}><a href="#">{{= i }}</a></li>
                <!--{{if ((i+1) == pageNumber){ }}-->
                <!--<li class="active" class="yemaanniu"><a href="#">{{= (i + 1).toString() }}<span class="sr-only">(current)</span></a></li>-->
                <!--{{ }else{ }}-->
                <!--<li class="yemaanniu"><a>{{= (i + 1).toString() }}<span class="sr-only">(current)</span></a></li>-->
                <!--{{ } }}-->
                {{ } }}
            </ul>
        </nav>
    </div>
</script>

